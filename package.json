{
  "name": "voter-management-system",
  "version": "0.1.0",
  "private": true,
  "description": "Voter Management System monorepo",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace api & npm run dev --workspace web",
    "dev:api": "npm run dev --workspace api",
    "dev:web": "npm run dev --workspace web",
    "build": "npm run build --workspace api && npm run build --workspace web",
    "lint": "npm run lint --workspace api && npm run lint --workspace web"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  },
  "overrides": {
    "path-scurry": {
      "lru-cache": "^10.2.0"
    }
  },
  "dependencies": {
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@types/pdf-parse": "^1.1.5"
  }
}
